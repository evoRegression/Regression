<Window x:Class="LinearRegressionWPF.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:LinearRegressionWPF.Views"
        xmlns:viewmodels="clr-namespace:LinearRegressionWPF.ViewModels" d:DataContext="{d:DesignInstance Type=viewmodels:MainWindowViewModel}"
        mc:Ignorable="d"
        Title="Linear Regression" Height="500" Width="1100">

    <TabControl>
        <TabItem Header="Regression model">
            <views:LinearRegressionTab/>
        </TabItem>
        <TabItem Header="Draw shape">
            <views:ImageToVectorTab/>
        </TabItem>
    </TabControl>

</Window>
